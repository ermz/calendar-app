<!DOCTYPE html>
<html lang="en">

  <head>
    <meta charset="utf-8">


    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>


    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Julius Sans One">

    <link rel="stylesheet" href="/css/app.css">
    <link rel="stylesheet" href="/css/header.css">
    <link rel="stylesheet" href="/css/login.css">
  </head>


  <body class="body">

    <%- include header.ejs %>

    <div class="login-status-div">
      <h3 class="login-status">hello</h3>
    </div>

    <div class="container sisi-div">
      <div class="row">

        <div class="login-div col-sm-6">
          <form name="login">
            <input name="login-username"></input>
            <input name="login-password"></input>
            <button class="sisi-button">Login</button>
          </form>
        </div>
        <div class="signin-div col-sm-6">
          <form name="signin">
            <input name="signin-username"></input>
            <input name="signin-password"></input>
            <button class="sisi-button">Sign up</button>
          </form>
        </div>

      </div>
    </div>




    <div class="logout-div">
      <form name="logout">
        <button>Log out</button>
      </form>
    </div>



    <%- include footer.ejs %>


    <script src="/js/login.js"></script>
    <script src="/js/header.js"></script>
  </body>



</html>
